{
  "name": "ecars-web-lib",
  "version": "1.0.14",
  "private": false,
  "main": "dist/index.cjs.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.cjs.js",
      "types": "./dist/index.d.ts"
    },
    "./styles": {
      "default": "./dist/assets/index.css"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "bunx --bun vite",
    "build": "tsc --p ./tsconfig-build.json && vite build",
    "build:dev": "tsc && vite build --mode development",
    "predeploy": "tsc --p ./tsconfig-build.json && vite build",
    "deploy": "gh-pages -d dist",
    "lint": "eslint lib --ext .js,.jsx,.ts,.tsx",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "lint:fix": "eslint lib --ext .js,.jsx,.ts,.tsx --fix",
    "prettier": "prettier lib --ext .js,.jsx,.ts,.tsx",
    "prettier:write": "prettier --write lib --ext .js,.jsx,.ts,.tsx",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@types/react-image-gallery": "^1.2.4",
    "clsx": "^2.1.1",
    "rc-slider": "^11.1.8",
    "react-image-gallery": "^1.4.0",
    "swiper": "^11.2.8"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "3.2.2",
    "@codecov/vite-plugin": "^1.9.0",
    "@eslint/compat": "^1.2.8",
    "@eslint/js": "^9.15.0",
    "@storybook/addon-actions": "^8.4.6",
    "@storybook/addon-essentials": "8.4.6",
    "@storybook/addon-interactions": "8.4.6",
    "@storybook/addon-links": "^8.4.6",
    "@storybook/addon-onboarding": "8.4.6",
    "@storybook/blocks": "8.4.6",
    "@storybook/react": "^8.4.6",
    "@storybook/react-vite": "8.4.6",
    "@storybook/test": "^8.6.12",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^24.2.1",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "^8.29.1",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/eslint-plugin": "^1.1.40",
    "ajv": "^8.17.1",
    "ajv-draft-04": "^1.0.0",
    "dart-sass": "^1.25.0",
    "dotenv": "^16.4.7",
    "esbuild-plugin-sass": "^1.0.1",
    "eslint": "^9.24.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-eslint-plugin": "^6.4.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jest": "^28.11.0",
    "eslint-plugin-jsdoc": "^50.6.9",
    "eslint-plugin-prettier": "^5.2.6",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-storybook": "^0.11.1",
    "gh-pages": "^6.3.0",
    "glob": "^11.0.1",
    "globals": "^15.12.0",
    "init": "^0.1.2",
    "jest-environment-jsdom": "^29.7.0",
    "jest-junit": "^16.0.0",
    "jiti": "^2.4.2",
    "jsdom": "^26.0.0",
    "prettier": "^3.4.2",
    "react-icons": "^5.5.0",
    "react-router": "^7.2.0",
    "storybook-addon-sass-postcss": "^0.3.2",
    "ts-node": "^10.9.2",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.15.0",
    "vite": "^6.2.5",
    "vite-plugin-dts": "^4.3.0",
    "vite-plugin-lib-inject-css": "^2.2.1",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
